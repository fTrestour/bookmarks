# Server Configuration
PORT=3000
HOST=localhost

# Database Configuration
DB_URL=file:sqlite/db.sqlite
DB_AUTH_TOKEN=

# OpenAI Configuration
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# AI Model Configuration
# AI model for content extraction
SCRAPING_AI_MODEL=gpt-5-mini
# AI model for generating embeddings
AI_EMBEDDING_MODEL=text-embedding-3-small

# AI model for generating descriptions
DESCRIPTION_GENERATION_AI_MODEL=gpt-5-mini

# JWT Configuration
JWT_SECRET=your_jwt_secret_here

# Rate Limiting
LIMIT=10

# CORS Configuration
# Comma-separated list of allowed origins for CORS (empty array allows all origins)
CORS_ORIGINS=

# Environment
NODE_ENV=development
